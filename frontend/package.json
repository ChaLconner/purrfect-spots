{
  "name": "purrfect-spots-frontend",
  "private": true,
  "version": "3.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:prod": "NODE_ENV=production vue-tsc && vite build --mode production",
    "build:analyze": "vite build --mode production && npx source-map-explorer dist/assets/js/*.js",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format": "prettier --write src/**/*.{ts,vue,css}",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^2.0.2",
    "@sentry/vue": "^8.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.7.7",
    "pinia": "^3.0.4",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.6.4",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@types/google.maps": "^3.58.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "terser": "^5.46.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^2.2.10"
  }
}
